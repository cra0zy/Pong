{
  "output": "$(OutDir)/",
  "outputFormatting": "Minified",
  "usetypedarrays": true,
  "console": {
      "enabled": true
  },
  "sourceMap": {
      "enabled": false
  },
  "reflection": {
      "disabled": false
  },
  "html": {
    "disabled": true
  },
  "generateTypeScript": false,
  "cleanOutputFolderBeforeBuild": false,
  "report": {
      "enabled": false
  },
  "rules": {
      "anonymousType": "Plain",
      "arrayIndex": "Plain",
      "autoProperty": "Plain",
      "boxing": "Plain",
      "integer": "Managed",
      "lambda": "Plain"
  }
}
